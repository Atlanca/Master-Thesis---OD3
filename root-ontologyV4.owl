    <!-- http://www.semanticweb.org/ontologies/snowflake#ArchitecturalPattern -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/snowflake#ArchitecturalPattern">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/snowflake#comprisesOf"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/ontologies/snowflake#Role"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/snowflake#resultOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/ontologies/snowflake#DesignOption"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Architectural patterns are common techniques used to solve recurring archititectural design problems.</dc:description>
    </owl:Class>
    


