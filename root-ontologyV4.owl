    <!-- http://www.semanticweb.org/ontologies/snowflake#update_item_in_cart -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/ontologies/snowflake#update_item_in_cart">
        <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/snowflake#FunctionalRequirement"/>
        <snowflake:partOf rdf:resource="http://www.semanticweb.org/ontologies/snowflake#purchase_products"/>
        <snowflake:performedBy rdf:resource="http://www.semanticweb.org/ontologies/snowflake#customer"/>
        <snowflake:Description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">As a customer, I want to change the number of units of a particular item in the shopping cart, so that I can buy a different quantity of the product with the next order</snowflake:Description>
