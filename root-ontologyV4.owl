                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/ontologies/snowflake#Role"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Architecture fragments are the parts of an architecture.</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is an abstract class. It individuals should not be directly related to this class. Instead, use one of its subclasses.</rdfs:comment>
    </owl:Class>
    


