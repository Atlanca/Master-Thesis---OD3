        <snowflake:modeledIn rdf:resource="http://www.semanticweb.org/ontologies/snowflake#figure_4.1_deployment"/>
        <snowflake:playsRole rdf:resource="http://www.semanticweb.org/ontologies/snowflake#data_server_tier_1"/>
        <snowflake:taggedBy rdf:resource="http://www.semanticweb.org/ontologies/snowflake#DatabaseServer"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SPHERE.IO Backend</rdfs:label>
    </owl:NamedIndividual>
    


