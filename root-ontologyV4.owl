    <!-- http://www.semanticweb.org/mahsaro/ontologies/2018/2/untitled-ontology-5#Forms -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/mahsaro/ontologies/2018/2/untitled-ontology-5#Forms">
        <rdf:type rdf:resource="http://www.semanticweb.org/mahsaro/ontologies/2018/2/untitled-ontology-5#ClassPackage"/>
