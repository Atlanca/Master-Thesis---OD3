    <!-- http://www.semanticweb.org/ontologies/snowflake#figure_4.14_browse_catalog_sequence -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/ontologies/snowflake#figure_4.14_browse_catalog_sequence">
